mixin head-data(title)
  meta(charset="UTF-8")
  meta(http-equiv="X-UA-Compatible", content="IE=edge")
  meta(name="viewport", content="width=device-width, initial-scale=1.0")
  title #{title}
  
  script(src="https://kit.fontawesome.com/4a2509d3c3.js" crossorigin="anonymous")
  
  link#favicon(rel="icon" type="image/png" href="img/favicon.png")
  
  link(rel="stylesheet", href="https://use.typekit.net/bkp1fmd.css")
  link(rel="stylesheet", href="css/main.css")
  //- link(rel="stylesheet", href="css/style.css")